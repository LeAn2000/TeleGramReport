@{
    ViewData["Title"] = "Report";
    Layout = "~/Views/Shared/Layout.cshtml";
}


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">

            <form class="form-inline">
                <div class="input-group mb-2 mr-sm-2"><input id="kd-place-chooser" /></div>
                <div class="input-group mb-2 mr-sm-2"><input type="date" class="form-control rounded1" placeholder="dd/mm/yyyy"></div>
                <button type="submit" class="btn btn-primary mb-2"> Submit </button>
            </form>

            <div id="tabstrip" kendo-tab-strip="tabstrip">
                <ul>
                    <li class="k-active">Tổng hợp</li>
                    <li>Chi tiết</li>
                </ul>
                <div>
                    <div id="grid">
                    </div>

                </div>
            </div>

        </div>
    </div>
</div>